      </LineChart>
      <PieChart fx:id="pieChart" layoutX="800.0" layoutY="38.0" />
      <PieChart fx:id="typePieChart" layoutX="160.0" layoutY="438.0" />
       <Button mnemonicParsing="false" onAction="#refresh" prefHeight="49.0" prefWidth="97.0" styleClass="green-theme,btn-radius" text="刷新" />
