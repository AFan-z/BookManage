          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </LineChart>
      <PieChart fx:id="pieChart" layoutX="800.0" layoutY="38.0" />
      <ButtonBar layoutX="210.0" layoutY="-6.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button onAction="#refresh" mnemonicParsing="false" text="刷新" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
