   <children>
      <LineChart layoutX="160.0" layoutY="38.0" fx:id="xyLineChart">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
