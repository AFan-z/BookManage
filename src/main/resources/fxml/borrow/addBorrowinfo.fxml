<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="174.0" prefWidth="507.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.demo.controller.borrow.AddBorrowController">
    <children>
        <HBox alignment="CENTER" minHeight="50" styleClass="green-theme" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
            <children>
                <Label styleClass="font-title,green-theme,font-white" text="新增借阅信息" />
            </children>
        </HBox>
        <VBox minHeight="227.0" prefHeight="227.0" prefWidth="507.0" spacing="20" styleClass="default-theme" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="55">
            <children>
            <ComboBox fx:id="jobNum" prefHeight="25.0" prefWidth="333.0" promptText="用户工号" />
            <ComboBox fx:id="bookNum" prefHeight="25.0" prefWidth="335.0" promptText="图书编号" />
            </children>
            <padding>
                <Insets bottom="10" left="50" right="50" top="10" />
            </padding>
        </VBox>
        <Button onAction="#addBorrow" styleClass="green-theme,btn-radius" text="确认" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="180" AnchorPane.rightAnchor="180" />
    </children>
</AnchorPane>
